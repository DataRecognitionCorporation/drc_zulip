{{! Stream sidebar rows }}

<li class="narrow-filter sub_sub_folder_li_{{subfolder_id}}" sub_subfolder_name="{{subfolder_name}}" subfolder_id="{{subfolder_id}}">
    <div id="subfolder_li_{{subfolder_id}}">
        <div class="subscription_block_subfolder sub_subfolder_{{subfolder_id}}">
            <div class="selectable_sidebar_block subfolder_underline stream_sub_subfolder_item">
                <span class="subfolder_span" title="{{subfolder_name}}">{{subfolder_name}}</span>
                <span class="sub_subfolder_unread_count unread_count"></span>
            </div>

            <ul id="subfolder_rows_{{subfolder_id}}" class="sub_subfolder_rows" subfolder_rows="{{subfolder_name}}"></ul>
        </div>
    </div>
</li>
