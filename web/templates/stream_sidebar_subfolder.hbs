{{! Stream sidebar rows }}

<li class="narrow-filter subfolder_{{subfolder_id}}" subfolder_name="{{subfolder_name}}" subfolder_id="{{subfolder_id}}">
    <div id="subfolder_li_{{subfolder_id}}">
        <div class="subscription_block_subfolder subfolder_{{subfolder_id}}">
            <div class="subfolder_underline stream_subfolder_item" >
                <span class="subfolder_span" title="{{subfolder_name}}">{{subfolder_name}}</span>
                <span class="subfolder_unread_count subfolder_unread_count_{{subfolder_id}} unread_count"></span>
            </div>

            <ul id="stream_sub_subfolder_{{subfolder_id}}" subfolder="{{subfolder_name}}"></ul>
        </div>
    </div>
</li>
